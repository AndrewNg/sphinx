{
  "manifest_version": 2,

  "name": "Headlok",
  "description": "This extension lets you login to sites using facial recognition",
  "version": "1.0",

  "permissions": ["tabs", "http://*/*"],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "options_page": "initialize.html"
}
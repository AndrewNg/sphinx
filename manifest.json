{
  "manifest_version": 2,

  "name": "Headlok",
  "description": "This extension lets you login to sites using facial recognition",
  "version": "1.0",

  "permissions": ["activeTab"],

  "background": {
    "background_page": ["background.html"]
  },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "options_page": "options.html"
}